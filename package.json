{
  "name": "e2e-santa",
  "version": "1.0.0",
  "description": "",
  "main": "cypress.config.js",
  "scripts": {
    "clear-cache": "rm -rf node_modules/.cache/cypress",
    "cy:run": "npx cypress run --headed --reporter mocha-allure-reporter",
    "allure:report": "allure generate allure-results --clean -o allure-report",
    "allure:clear": "rm -r allure-report/* & rm -r allure-results/*",
    "report:open": "allure serve allure-results",
    "test:with-allure": "npm run allure:clear & npm run cy:run & npm run allure:report & sleep 5 && npm run report:open"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@badeball/cypress-cucumber-preprocessor": "^20.0.3",
    "@faker-js/faker": "^7.6.0"
  },
  "devDependencies": {
    "@bahmutov/cypress-esbuild-preprocessor": "^2.2.0",
    "@shelex/cypress-allure-plugin": "^2.40.2",
    "cypress": "^13.7.3",
    "esbuild": "^0.20.2",
    "mocha-allure-reporter": "^1.4.0",
    "mocha-junit-reporter": "^2.2.1"
  }
}